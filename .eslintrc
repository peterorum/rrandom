{
  "extends": "eslint:recommended",
  "plugins": ["react"],
  "ecmaFeatures": {
              "jsx": true,
              "es6": true,
              "modules": true,
              "browser": true,
              "jasmine": true
  },
  "globals": {
  "jest": false,
  "React": true
  },
  "env": {
      "node": true,
      "jasmine": true,
      "browser": true
  },
  "rules": {
    "react/jsx-quotes": 0,
    "jsx-quotes": [1, "prefer-single"],
    "no-unused-vars": [1, {"varsIgnorePattern": "React"}]
  }
}
